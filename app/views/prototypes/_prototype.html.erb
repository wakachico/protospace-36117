<div class="card">
  <%#= image_tag prototype.image.variant(resize: '500x500'), class: :card__img %>
  <%#= link_to image_tag prototype.image, class: :card__img , root_path%>
  <%= link_to image_tag(prototype.image.variant(resize: '300x300'), class: :card__img ), prototype_path(prototype.id) %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title%>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{prototype.user.name}", user_path(prototype.user.id), class: :card__user %>
  </div>
</div>